{
  "name": "cypress",
  "version": "1.0.0",
  "description": "cypress end-to-end test automation boilerplate project.",
  "scripts": {
    "clean:video": "npx rimraf cypress/videos/*",
    "clean:screenshots": "npx cypress/screenshots/*",
    "open": "npx cypress open",
    "test": "npx cypress run",
    "test-ci": "npx cypress run --headless",
    "lint": "npx eslint --ext .js ./ --fix"
  },
  "keywords": [
    "cypress",
    "boilerplate",
    "example"
  ],
  "author": "Girma Nigusse <xgirma@gmail.com> (https://github.com/xgirma)",
  "license": "MIT",
  "devDependencies": {
    "cypress": "3.8.3",
    "eslint": "6.8.0",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-plugin-cypress": "2.8.1",
    "eslint-plugin-import": "2.20.0",
    "eslint-plugin-prettier": "3.1.2",
    "husky": "4.2.1",
    "lint-staged": "10.0.3",
    "prettier": "1.19.1",
    "rimraf": "3.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  }
}
